    "browser-sync": "^2.26.12",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "http-server": "^0.12.3",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.1.1",
    "pug": "^3.0.0",
    "terser-webpack-plugin": "^5.0.0",
